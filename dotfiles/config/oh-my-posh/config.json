{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/refs/tags/v24.11.4/themes/schema.json",
  "palette": {
    "rose-pine-main-base": "#191724",
    "rose-pine-main-surface": "#1f1d2e",
    "rose-pine-main-overlay": "#26233a",
    "rose-pine-main-muted": "#6e6a86",
    "rose-pine-main-subtle": "#908caa",
    "rose-pine-main-text": "#e0def4",
    "rose-pine-main-love": "#eb6f92",
    "rose-pine-main-gold": "#f6c177",
    "rose-pine-main-rose": "#ebbcba",
    "rose-pine-main-pine": "#31748f",
    "rose-pine-main-foam": "#9ccfd8",
    "rose-pine-main-iris": "#c4a7e7",
    "rose-pine-main-highlight-low": "#6e6a86",
    "rose-pine-main-highlight-med": "#908caa",
    "rose-pine-main-highlight-high": "#c4a7e7"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:rose-pine-main-gold",
          "template": " \u279C"
        },
        {
          "type": "kubectl",
          "style": "plain",
          "foreground": "p:rose-pine-main-iris",
          "template": " \udb84\udcfe {{.Context}}({{.Namespace}})"
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:rose-pine-main-iris",
          "template": " {{ .HEAD }}"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:rose-pine-main-iris",
          "template": " \uE235 {{ .Venv }}",
          "properties": {
            "home_enabled": true
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "p:rose-pine-main-foam",
          "template": " \uF115 {{ .Path }}",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "p:rose-pine-main-love",
          "template": " \u2717"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:rose-pine-main-iris",
          "template": " $"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
